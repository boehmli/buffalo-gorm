<div class="page-header">
  <h1>{{.Model.Model}}#Show</h1>
</div>

<ul class="list-unstyled list-inline">
  <li class="list-inline-item"><a href="<%= {{.Name.VarCasePlural}}Path() %>" class="btn btn-info">Back to all {{.Model.ModelPlural}}</a></li>
  <li class="list-inline-item"><a href="<%= edit{{.Name.Model}}Path({ {{.Name.ParamID}}: {{.Model.VarCaseSingular}}.ID })%>" class="btn btn-warning">Edit</a></li>
  <li class="list-inline-item"><a href="<%= {{.Name.VarCaseSingular}}Path({ {{.Name.ParamID}}: {{.Model.VarCaseSingular}}.ID })%>" data-method="DELETE" data-confirm="Are you sure?" class="btn btn-danger">Destroy</a>
</ul>

{{ range $p := .Props -}}
<p>
  <strong>{{ $p.Name }}</strong>: <%= {{$.Model.VarCaseSingular}}.{{$p.Name}} %>
</p>
{{ end -}}

